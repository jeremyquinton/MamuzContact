{
  "name": "mamuz/mamuz-contact",
  "type": "library",
  "description": "Provides simple contact form rendered by twitter-bootstrap view helper for ZF2 with Doctrine",
  "homepage": "https://github.com/mamuz/MamuzContact",
  "license": "MIT",
  "keywords": [
    "contact",
    "form",
    "zf2",
    "twitter-bootstrap",
    "doctrine"
  ],
  "authors": [
    {
      "name": "Marco Muths",
      "email": "mamuz.de@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.4",
    "zendframework/zend-mvc": "*",
    "zendframework/zend-code": "*",
    "zendframework/zend-session": "*",
    "zendframework/zend-servicemanager": "*",
    "zendframework/zend-eventmanager": "*",
    "zendframework/zend-modulemanager": "*",
    "zendframework/zend-view": "*",
    "zendframework/zend-http": "*",
    "zendframework/zend-form": "*",
    "zendframework/zend-stdlib": "*",
    "zendframework/zend-i18n": "*",
    "zendframework/zend-captcha": "*",
    "zendframework/zend-filter": "*",
    "zendframework/zend-validator": "*",
    "doctrine/doctrine-orm-module": "*",
    "neilime/zf2-twb-bundle": "*",
    "zendframework/zendservice-recaptcha": "*"
  },
  "require-dev": {
    "satooshi/php-coveralls": "0.6.*",
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.9.*",
    "mamuz/php-dependency-analysis": "0.*"
  },
  "autoload": {
    "psr-0": {
      "MamuzContact\\": "src/"
    }
  }
}
